    "@types/date-fns":"^2.6.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.191",
    "@types/md5": "^2.3.1",
    "@types/node": "^14.14.28",
    "@types/utf8": "^3.0.0",
